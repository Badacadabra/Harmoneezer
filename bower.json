{
  "name": "HARMONEEZER",
  "description": "Application web d'aide à la création de playlists harmoniques",
  "main": "server.js",
  "authors": [
    "Baptiste Vannesson"
  ],
  "license": "MIT",
  "keywords": [
    "music",
    "playlist",
    "harmony"
  ],
  "homepage": "https://github.com/Badacadabra/Harmoneezer",
  "moduleType": [
    "node"
  ],
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "jquery": "~2.2.0",
    "jquery-ui": "jqueryui#~1.11.4",
    "vue": "~1.0.17",
    "underscore": "~1.8.3",
    "owlcarousel": "~1.3.2",
    "alertify-js": "~1.6.1",
    "malihu-custom-scrollbar-plugin": "~3.1.3",
    "tabletoCSV": "~0.1.0",
    "qtip2": "~2.2.1",
    "vegas": "~2.1.3"
  },
  "overrides": {
    "jquery": {
      "main": "**/jquery.min.js"
    },
    "jquery-ui": {
      "main": "**/jquery-ui.min.js"
    },
    "vue": {
      "main": "**/vue.min.js"
    },
    "underscore": {
      "main": "**/underscore-min.js"
    },
    "owlcarousel": {
      "main": ["**/owl.carousel.min.js", "**/owl.carousel.css", "**/owl.theme.css"]
    },
    "alertify-js": {
      "main": ["**/alertify.min.js", "**/alertify.min.css", "**/default.min.css"]
    },
    "malihu-custom-scrollbar-plugin": {
      "main": ["**/jquery.mCustomScrollbar.concat.min.js", "**/jquery.mCustomScrollbar.min.css"]
    },
    "tabletoCSV": {
      "main": "**/jquery.tabletoCSV.js"
    },
    "qtip2": {
      "main": ["**/jquery.qtip.min.js", "**/jquery.qtip.min.css"]
    },
    "vegas": {
      "main": ["**/vegas.min.js", "**/vegas.min.css"]
    }
  },
  "devDependencies": {
    "qunit": "~1.20.0",
    "jquery-bridget": "~2.0.0"
  }
}
